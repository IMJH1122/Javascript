let x = 10;
let y = 15;
let z = 20;
let w = 25;

function setup() {
  createCanvas(windowWidth, windowHeight);
  background(220);
}

function draw() {
  print(mouseIsPressed);
  print(keyIsPressed);
  if (x != y) {
    if (x <= y) {
      print("<");
      x += 1;
    }
    else if (x >= y) {
      print(">");
      x -= 1;
    }
  }
  if(x > y === z > w && mouseIsPressed) {
    if(mouseButton == LEFT) {
      stroke(2);
      fill(255);
      ellipse(random(width), random(height), 50, 50);
    }
    else if(mouseButton == CENTER){
      stroke(2);
      fill(255);
      circle(mouseX, mouseY, 100);
    }
    else if(mouseButton == RIGHT){
      noStroke();
      fill(220);
      circle(mouseX, mouseY, 50);
    }
  }
}
